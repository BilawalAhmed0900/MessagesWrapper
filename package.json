{
  "name": "messages",
  "version": "1.0.1",
  "description": "A messages webview in Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . \"Messages for Web\" --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Messages for Web\"",
    "package-linux": "electron-packager . \"Messages for Web\" --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "BilawalAhmed0900",
  "license": "ISC",
  "devDependencies": {
    "electron": "^18.0.4",
    "electron-packager": "^15.4.0"
  }
}
